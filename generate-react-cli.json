{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "none",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/components/TemplateName.tsx",
        "style": "templates/components/TemplateName.module.scss",
        "index": "templates/components/index.ts"
      },
      "path": "src/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false,
      "withIndex": true
    },
    "page": {
      "path": "src/pages",
      "withLazy": true,
      "withStory": false,
      "withStyle": true,
      "withTest": true
    },
    "layout": {
      "path": "src/layout",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": true
    }
  },
  "usesStyledComponents": false
}